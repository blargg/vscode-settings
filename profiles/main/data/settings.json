{
  "terminal.integrated.commandsToSkipShell": ["language-julia.interrupt"],
  "julia.symbolCacheDownload": true,
  "git.enableSmartCommit": true,
  "git.confirmSync": false,
  "julia.enableTelemetry": false,
  "settingsSync.keybindingsPerPlatform": false,
  "scm.defaultViewMode": "tree",
  "vim.normalModeKeyBindings": [
    {
      "before": ["<C-l>"],
      "commands": ["workbench.action.focusNextGroup"]
    },
    {
      "before": ["<space>", "r"],
      "commands": ["workbench.action.openRecent"]
    },
    {
      "before": ["<C-h>"],
      "commands": ["workbench.action.focusPreviousGroup"]
    },
    {
      "before": ["<space>", "w"],
      "commands": ["workbench.action.closeActiveEditor"]
    },
    {
      "before": ["<space>", "i"],
      "commands": ["editor.action.quickFix"]
    },
    {
      "before": ["K"],
      "commands": ["editor.action.showHover"]
    },
    {
      "before": ["<C-k>"],
      "commands": ["editor.action.showHover"]
    },
    {
      "before": ["<space>", "f"],
      "commands": ["workbench.action.showAllEditorsByMostRecentlyUsed"]
    },
    {
      "before": ["]", "g"],
      "commands": ["editor.action.marker.next"]
    },
    {
      "before": ["[", "g"],
      "commands": ["editor.action.marker.prev"]
    },
    {
      "before": ["[", "c"],
      "when": "editorFocus && !isInDiffEditor",
      "commands": ["workbench.action.editor.previousChange"]
    },
    {
      "before": ["]", "c"],
      "when": "editorFocus && !isInDiffEditor",
      "commands": ["workbench.action.editor.nextChange"]
    },
    {
      "before": ["<space>", "a", "g"],
      "commands": ["continue.focusEdit"]
    },
    {
      "before": ["<space>", "a", "y"],
      "commands": ["continue.acceptDiff"]
    },
    {
      "before": ["<space>", "a", "n"],
      "commands": ["continue.rejectDiff"]
    },
    {
      "before": ["<space>", "a", "d"],
      "commands": ["continue.writeDocstringForCode"]
    },
  ],
  "terminal.integrated.shell.windows": "C:\\Windows\\System32\\cmd.exe",
  "rust-client.engine": "rust-analyzer",
  "todo-tree.ripgrep.ripgrep": "/run/current-system/sw/bin/rg",
  "markdown.experimental.validate.enabled": true,
  "vim.visualModeKeyBindings": [
    {
      "before": ["g", "q"],
      "commands": ["editor.action.formatSelection"]
    },
    {
      "before": ["<space>", "a", "g"],
      "commands": ["continue.focusEdit"]
    },
    {
      "before": ["<space>", "a", "y"],
      "commands": ["continue.acceptDiff"]
    },
    {
      "before": ["<space>", "a", "n"],
      "commands": ["continue.rejectDiff"]
    },
    {
      "before": ["<space>", "a", "c"],
      "commands": ["continue.focusContinueInputWithoutClear"]
    },
    {
      "before": ["<space>", "a", "d"],
      "commands": ["continue.writeDocstringForCode"]
    },
  ],
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "explorer.confirmDragAndDrop": false,
  "editor.fontLigatures": false,
  "catppuccin.accentColor": "sky",
  "workbench.colorTheme": "Visual Studio Dark",
  "nix.serverPath": "nil",
  "nix.enableLanguageServer": true,
  "nix.formatterPath": "/run/current-system/sw/bin/nixpkgs-fmt",
  "[python]": {
    "editor.formatOnType": true
  },
  "telemetry.telemetryLevel": "off",
  "gitlens.telemetry.enabled": false,
  "editor.renderWhitespace": "all",
  "files.associations": {
    "*.hujson": "jsonc"
  },
  "json.schemas": [
    {
      "fileMatch": ["*.hujson"],
      "schema": {
        "allowTrailingCommas": true
      }
    }
  ],
  "editor.renderLineHighlight": "all",
  "nix.serverSettings": {
    "nil": {
      "formatting": {
        "command": ["nixpkgs-fmt"]
      }
    }
  },
  "rust-analyzer.diagnostics.disabled": ["unresolved-proc-macro"],
  "search.useGlobalIgnoreFiles": true,
  "cSpell.userWords": ["Jankauski", "tomjankauski"],
  "editor.formatOnSave": true,
  "vim.handleKeys": {
    "<C-p>": false,
    "<C-d>": true,
    "<C-s>": false,
    "<C-z>": false
  },
  "continue.telemetryEnabled": false,
  "continue.enableTabAutocomplete": false,
  "haskell.manageHLS": "GHCup",
  "continue.pauseTabAutocompleteOnBattery": true,
  "vim.insertModeKeyBindings": [
    {
      "before": ["<C+i>"],
      "commands": ["continue.focusEdit"]
    },
  ],
  "python.analysis.typeCheckingMode": "standard",
  "continue.enableQuickActions": true,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cronTasks.debug": false
}
